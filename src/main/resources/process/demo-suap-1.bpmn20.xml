<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <collaboration id="Collaboration">
    <participant id="pool1" processRef="SUAPdemo"></participant>
  </collaboration>
  <process id="SUAPdemo" name="SCUAP Demo" isExecutable="true">
    <laneSet id="laneSet_SUAPdemo">
      <lane id="lane1" name="SUAP Office">
        <flowNodeRef>startevent1</flowNodeRef>
        <flowNodeRef>usertask1</flowNodeRef>
        <flowNodeRef>usertask2</flowNodeRef>
        <flowNodeRef>usertask3</flowNodeRef>
        <flowNodeRef>usertask4</flowNodeRef>
        <flowNodeRef>endevent1</flowNodeRef>
        <flowNodeRef>exclusivegateway2</flowNodeRef>
        <flowNodeRef>usertask6</flowNodeRef>
        <flowNodeRef>endevent2</flowNodeRef>
        <flowNodeRef>usertask7</flowNodeRef>
        <flowNodeRef>endevent3</flowNodeRef>
        <flowNodeRef>usertask8</flowNodeRef>
        <flowNodeRef>applicationAdmissibleGateway</flowNodeRef>
      </lane>
      <lane id="lane2" name="Other Office">
        <flowNodeRef>usertask5</flowNodeRef>
      </lane>
    </laneSet>
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="entrepreneur" name="Entrepreneur" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="document" name="Entrepreneur document" type="enum" required="true">
          <activiti:value id="example_document_valid" name="Valid document"></activiti:value>
          <activiti:value id="example_document_invalid" name="Invalid document"></activiti:value>
        </activiti:formProperty>
      </extensionElements>
    </startEvent>
    <userTask id="usertask1" name="Deep Data Check" activiti:candidateGroups="SUAP">
      <documentation>${entrepreneur} has sent the following Application:
&lt;a href='resources/files/${document}.pdf' target='_blank'&gt;Entrepreneur document&lt;/a&gt;</documentation>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="usertask1"></sequenceFlow>
    <userTask id="usertask2" name="SCIA Application Verification" activiti:candidateGroups="SUAP">
      <documentation>You need to confirm that the application is admissible.</documentation>
      <extensionElements>
        <activiti:formProperty id="applicationAdmissible" name="Is the application admissible?" type="enum" required="true">
          <activiti:value id="true" name="Admissible"></activiti:value>
          <activiti:value id="false" name="Not admissible"></activiti:value>
        </activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="usertask1" targetRef="usertask2"></sequenceFlow>
    <exclusiveGateway id="applicationAdmissibleGateway" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow3" sourceRef="usertask2" targetRef="applicationAdmissibleGateway"></sequenceFlow>
    <userTask id="usertask3" name="Send Communication of non-admissibility" activiti:candidateGroups="SUAP">
      <documentation>Inform ${entrepreneur} of the application non-admissibility.</documentation>
      <extensionElements>
        <activiti:formProperty id="applicationRejectionMsg" name="Application Rejection Message" type="string" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow4" name="application non admissible" sourceRef="applicationAdmissibleGateway" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${applicationAdmissible == 'false'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask4" name="Send Communication Starting Check" activiti:candidateGroups="SUAP">
      <documentation>Inform ${entrepreneur} of the beginning of application check.</documentation>
      <extensionElements>
        <activiti:formProperty id="applicationCheckBeginMsg" name="Application Check Begin Message" type="string" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow5" name="application admissible" sourceRef="applicationAdmissibleGateway" targetRef="usertask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${applicationAdmissible == 'true'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="usertask4" targetRef="usertask5"></sequenceFlow>
    <userTask id="usertask5" name="Do Check" activiti:candidateGroups="otherOffice">
      <documentation>The following document need to be checked for validation:
&lt;a href='resources/files/${document}.pdf' target='_blank'&gt;Entrepreneur document&lt;/a&gt;</documentation>
      <extensionElements>
        <activiti:formProperty id="documentValid" name="Is the document valid?" type="enum" required="true">
          <activiti:value id="true" name="Valid"></activiti:value>
          <activiti:value id="false" name="Invalid"></activiti:value>
        </activiti:formProperty>
      </extensionElements>
    </userTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow7" sourceRef="usertask3" targetRef="endevent1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <userTask id="usertask6" name="Send inhibition" activiti:candidateGroups="SUAP">
      <documentation>Inform ${entrepreneur} of the application rejection.</documentation>
      <extensionElements>
        <activiti:formProperty id="applicationRejectionMsg" name="Application Rejection Message" type="string" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow9" name="application not validated" sourceRef="exclusivegateway2" targetRef="usertask6">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${applicationValid == 'false'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow10" sourceRef="usertask6" targetRef="endevent2"></sequenceFlow>
    <userTask id="usertask7" name="Send Authorization Documents" activiti:candidateGroups="SUAP">
      <documentation>Send the authorization to ${entrepreneur}.</documentation>
      <extensionElements>
        <activiti:formProperty id="authorizationMsg" name="Application Authorization Document" type="string" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow11" name="application validated" sourceRef="exclusivegateway2" targetRef="usertask7">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${applicationValid == 'true'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent3" name="End"></endEvent>
    <sequenceFlow id="flow12" sourceRef="usertask7" targetRef="endevent3"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="usertask5" targetRef="usertask8"></sequenceFlow>
    <userTask id="usertask8" name="Validate Application" activiti:candidateGroups="SUAP">
      <documentation>Here are the result of the documents check for the application of ${entrepreneur}:
			Documents are valid: ${documentValid}</documentation>
      <extensionElements>
        <activiti:formProperty id="applicationValid" name="Do you validate the application?" type="enum" required="true">
          <activiti:value id="true" name="Valid"></activiti:value>
          <activiti:value id="false" name="Invalid"></activiti:value>
        </activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow14" sourceRef="usertask8" targetRef="exclusivegateway2"></sequenceFlow>
  </process>
